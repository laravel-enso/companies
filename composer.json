{
    "name": "laravel-enso/companies",
    "description": "Person management dependency for Laravel Enso",
    "keywords": [
        "laravel",
        "laravel-enso",
        "companies",
        "company-management"
    ],
    "homepage": "https://github.com/laravel-enso/companies",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-enso/core": "^9.0",
        "laravel-enso/addresses": "^5.0",
        "laravel-enso/enums": "^2.0",
        "laravel-enso/dynamic-methods": "^3.0",
        "laravel-enso/forms": "^4.0",
        "laravel-enso/helpers": "^2.0",
        "laravel-enso/migrator": "^2.1",
        "laravel-enso/people": "^4.0",
        "laravel-enso/rememberable": "^3.0",
        "laravel-enso/searchable": "^2.2",
        "laravel-enso/track-who": "^2.0",
        "laravel-enso/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Companies\\": "src/",
            "LaravelEnso\\Companies\\Database\\Factories\\": "database/factories/",
            "LaravelEnso\\Companies\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Companies\\AppServiceProvider",
                "LaravelEnso\\Companies\\AuthServiceProvider",
                "LaravelEnso\\Companies\\SearchServiceProvider",
                "LaravelEnso\\Companies\\EnumServiceProvider"
            ],
            "aliases": {}
        }
    }
}
